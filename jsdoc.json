{
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  },
  "docdash": {
    "static": false,
    "sort": true
  },
  "source": {
    "includePattern": ".*\\\\.+\\.js(doc|x)?$",
    "include": [
      "lib",
      "./README.md",
      "./index.js"
    ]
  }
}
